  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development dotenv -e .env.development -- next dev",
    "build": "cross-env NODE_ENV=production dotenv -e .env.production -- next build",
    "start": "cross-env NODE_ENV=production dotenv -e .env.production -- next start",
    "lint": "next lint",
    "db:seed": "tsx prisma/seed/index.ts",
    "db:seed:dev": "cross-env NODE_ENV=development dotenv -e .env.development -- tsx prisma/seed/index.ts",
    "db:seed:prod": "cross-env NODE_ENV=production dotenv -e .env.production -- tsx prisma/seed/index.ts",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "cross-env NODE_ENV=development dotenv -e .env.development -- prisma migrate deploy",
    "db:migrate:prod": "cross-env NODE_ENV=production dotenv -e .env.production -- prisma migrate deploy",
    "db:migrate:test": "cross-env NODE_ENV=test dotenv -e .env.test -- prisma migrate deploy",
    "db:migrate:create": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "dotenv -e .env.development -- prisma studio",
    "db:studio:dev": "cross-env NODE_ENV=development dotenv -e .env.development -- prisma studio",
    "db:studio:prod": "cross-env NODE_ENV=production dotenv -e .env.production -- prisma studio",
    "db:studio:test": "cross-env NODE_ENV=test dotenv -e .env.test -- prisma studio",
    "db:status": "dotenv -e .env.development -- prisma migrate status",
    "db:status:dev": "cross-env NODE_ENV=development dotenv -e .env.development -- prisma migrate status",
    "db:status:prod": "cross-env NODE_ENV=production dotenv -e .env.production -- prisma migrate status",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f postgres",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "test": "cross-env NODE_ENV=test dotenv -e .env.test -- jest",
    "download-fonts": "node scripts/download-outfit-font.js"
  },
  "prisma": {
    "seed": "tsx prisma/seed/index.ts",
